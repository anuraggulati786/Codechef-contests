                                             ############    Hardest Problem Bet      ###############
											 
											 
											 
											 
											 
#include <iostream>
using namespace std;

int main() 
{      int i;
       int    Sa[100],Sb[100],Sc[100];
    
       int T;
       cin>>T;
       
       for(i=0;i<T;i++)
       {
       	   
			  
			  	cin>>Sa[i];
			    cin>>Sb[i];
			  	cin>>Sc[i];
       	      
	   }
	   
	   for(i=0;i<T;i++)
	   {
	   	     if((Sc[i]<Sa[i])&&(Sc[i]<Sb[i]))
              {
                     cout<<"Alice"<<endl;
              }
              else if((Sb[i]<Sa[i])&&(Sb[i]<Sc[i]))
              {
                     cout<<"Bob"<<endl;
              }
              else
              {
              	cout<<"Draw"<<endl;
			  }
	   }
   
}
